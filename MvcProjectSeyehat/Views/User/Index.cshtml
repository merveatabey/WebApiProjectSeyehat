        <!-- Travels Section -->
        <section id="travels" class="travels section">
            <div class="container section-title">
                <h2>Seyahatler</h2>
                <p>Popüler Seyahatlerimiz<br></p>
            </div><!-- End Section Title -->

            <div class="container">
                <div id="travelsCarousel" class="carousel slide" data-bs-ride="carousel">
                    <div class="carousel-inner">
                        @for (int i = 0; i < Model.Travels.Count; i += 1)
                        {
                            var currentGroup = Model.Travels.Skip(i).Take(4).ToList();
                            <div class="carousel-item @(i == 0 ? "active" : "")">
                                <div class="row gy-4">
                                    @foreach (var trip in currentGroup)
                                    {
                                        <div class="col-xl-3 col-md-6">
                                            <div class="service-item">
                                                <div class="img">
                                                    <img src="@trip.TripImg" class="img-fluid" alt="@trip.TripName" style="width: 100%; height: 200px; object-fit: cover;">
                                                </div>
                                                <div class="details position-relative">
                                                    <div class="icon">
                                                        <i class="bi bi-activity"></i>
                                                    </div>
                                                    <a href="service-details.html" class="stretched-link">
                                                        <h3>@trip.TripName</h3>
                                                    </a>
                                                    <p>@trip.Notes</p>
                                                    <p>@trip.StartDate.ToString("d") - @trip.EndDate.ToString("d")</p>
                                                    <a class="cta-btn" href="index.html#about">Detay</a>
                                                </div>
                                            </div>
                                        </div>

                                    }
                                </div>
                            </div>
                        }
                    </div>

                    <!-- Carousel Controls -->
                    <button class="carousel-control-prev" type="button" data-bs-target="#travelsCarousel" data-bs-slide="prev" style="position: absolute; top: 50%; left: 0; z-index: 10; transform: translateY(-50%);">
                        <span class="carousel-control-prev-icon" aria-hidden="true"></span>
                        <span class="visually-hidden">Previous</span>
                    </button>
                    <button class="carousel-control-next" type="button" data-bs-target="#travelsCarousel" data-bs-slide="next" style="position: absolute; top: 50%; right: 0; z-index: 10; transform: translateY(-50%);">
                        <span class="carousel-control-next-icon" aria-hidden="true"></span>
                        <span class="visually-hidden">Next</span>
                    </button>
                </div>
            </div>
        </section><!-- /Seyahatler Section -->
      

